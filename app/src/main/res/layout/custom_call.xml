<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp"
    android:background="#efefef">

    <TextView
        android:id="@+id/c_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:hint="Call Log Name"/>
    <TextView
        android:id="@+id/c_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:hint="Call Log Number"/>
    <TextView
        android:id="@+id/c_timeStamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:hint="TimeStamp"/>
    <TextView
        android:id="@+id/c_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:hint="Duration seconds"/>
    <TextView
        android:id="@+id/c_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:hint="Call Type"/>

</LinearLayout>